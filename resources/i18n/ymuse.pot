"{{- else if .Name -}}\n"
"<big><b>{{ .Name }}</b></big>\n"
"{{- else if .file -}}\n"
"File <big><b>{{ .file | basename }}</b></big>\n"
"from <b>{{ .file | dirname }}</b>\n"
"{{- else -}}\n"
"<i>(no track)</i>\n"
"{{- end -}}\n"
