msgid ""
"{{- if or .Title .Album | or .Artist -}}\n"
"<big><b>{{ .Title | default \"(unknown title)\" }}</b></big>\n"
"by <b>{{ .Artist | default \"(unknown artist)\" }}</b> from <b>{{ .Album | default \"(unknown album)\" }}</b>\n"
"{{- else if .Name -}}\n"
"<big><b>{{ .Name }}</b></big>\n"
"{{- else if .file -}}\n"
"File <big><b>{{ .file | basename }}</b></big>\n"
"from <b>{{ .file | dirname }}</b>\n"
"{{- else -}}\n"
"<i>(no track)</i>\n"
"{{- end -}}\n"
msgstr ""
"{{- if or .Title .Album | or .Artist -}}\n"
"<big><b>{{ .Title | default \"(unbekanntes Stück)\" }}</b></big>\n"
"von <b>{{ .Artist | default \"(unbekanntem Künstler)\" }}</b> von <b>{{ .Album | default \"(unbekanntem Album)\" }}</b>\n"
"{{- else if .Name -}}\n"
"<big><b>{{ .Name }}</b></big>\n"
"{{- else if .file -}}\n"
"File <big><b>{{ .file | basename }}</b></big>\n"
"from <b>{{ .file | dirname }}</b>\n"
"{{- else -}}\n"
"<i>(kein Stück)</i>\n"
"{{- end -}}\n"

